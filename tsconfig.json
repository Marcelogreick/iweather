{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "strictNullChecks": false,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@libs/*": [
        "./src/libs/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@src/*": [
        "./src/*"
      ],
      "@__tests__/*": [
        "__tests__/*"
      ],
      "react": [
        "./node_modules/@types/react"
      ]
    },
    "typeRoots": ["./node_modules/@types"]
  }
}